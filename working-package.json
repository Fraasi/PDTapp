    "@fullcalendar/list": "^4.2.0",
    "@fullcalendar/moment": "^4.2.0",
    "@fullcalendar/react": "^4.2.0",
    "@fullcalendar/timegrid": "^4.2.0",
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^8.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-store": "^5.1.0",
    "futility": "github:fraasi/futility",
    "moment": "^2.24.0",
    "node-pty": "^0.9.0-beta9",
    "react": "^16.8.6",
