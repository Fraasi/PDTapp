{
  "name": "pdtapp",
  "productName": "PDTapp",
  "version": "2.0.0",
  "description": "Personal Desktop app",
  "author": "Fraasi <fraasi.gh@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/fraasi/PDTapp.git"
  },
  "main": "app/main.js",
  "scripts": {
    "start": "npm-run-all --parallel bundle serve",
    "bundle": "cross-env NODE_ENV=development webpack --mode development",
    "build:production": "cross-env NODE_ENV=production webpack --mode production",
    "serve": "electron .",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "install-hooks": "cp -v bin/* .git/hooks/",
    "rebuild": "electron-rebuild",
    "rebuild-pty": "electron-rebuild -f -w node-pty"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
