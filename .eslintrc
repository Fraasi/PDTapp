{
  "extends": "eslint-config-airbnb",
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [2, { "ignore": ["electron"] }],
    "linebreak-style": 0,
		"react/prefer-stateless-function": 0,
		"no-console": 0,
		"no-tabs": 0,
		"indent": 0,
		"global-require": 0,
		"react/jsx-indent": 0,
		"import/newline-after-import": 0,
		"semi":0,
		"space-before-function-paren": 0,
		"comma-dangle": 0,
		"consistent-return": 0,
		"no-alert": 0
  }
}
